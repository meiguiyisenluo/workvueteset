    "js-cookie": "^2.2.1",
    "js-md5": "^0.7.3",
    "jsencrypt": "^3.0.0-rc.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "libpag": "^4.2.84",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "normalize.css": "^8.0.1",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.3",
    "qs": "^6.10.1",
    "sm4util": "^1.0.5",
    "store2": "^2.12.0",
    "style-loader": "^1.3.0",
    "vant": "^2.12.0",
    "vue": "^2.6.11",
    "vue-clipboard2": "^0.3.1",
    "vue-router": "^3.4.5",
