{
  "name": "cloudappweb",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "stylelint": "stylelint --fix './**/*.{html,vue,less}'",
    "prettier": "prettier --write './**/*.{js,css,less,vue,json}'"
  },
  "dependencies": {
    "@vant/touch-emulator": "^1.4.0",
